<% if user_signed_in? %>
  Hello, <%= current_user.email %>.
  <%= link_to('Logout', destroy_user_session_path, :method => :delete) %>        
<% else %>
  You are not signed in. 
  <%= link_to('Login', new_user_session_path)  %>  <br />
  <%= link_to('Register', new_user_registration_path)  %>
<% end %>
